/* pages/about/about.wxss */
#content{
  width: 100%;
  height: 100%;
  background-color: #f2fcfd;
  padding: 2px;
}
#content #header{
  width: 100%;
  height: 130px;
  border-bottom: 1px solid silver;
  border-radius: 10px;
  background-color: aliceblue;
  box-shadow: hsl(0, 0%, 92%) 1px 1px 1px 1px;
}
#content #header #title{
  font-style: normal;
  font-size: 25px;
  padding: 9px;
}
#content #header #hbtn{
  width: 99%;
  height: 45px;
  background-color: transparent;
  border: none;
  border-radius: 0px;
  margin: auto;
}

#content #body{
  width: 100%;
  height: 300px;
  border: none;
}
#content #body .btn{
  border:none;
  border-radius: 0px;
  background-color: transparent;
  width: 100%;
  height: 50px;
  margin-top: 2px;
}
#content #body #btn1{
  margin-top: 70px;
}